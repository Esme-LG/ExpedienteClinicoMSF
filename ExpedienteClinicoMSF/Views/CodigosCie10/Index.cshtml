@model IEnumerable<ExpedienteClinicoMSF.Models.CodigosCie10>

@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Cie10)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NomEnfermedad)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Cie10)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NomEnfermedad)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.CodigoId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.CodigoId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.CodigoId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
